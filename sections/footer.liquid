                                    <path d="M9.5 0C4.2623 0 0 4.2623 0 9.5C0 11.7459 0.79235 13.9098 2.23771 15.6202L0.551913 18.9071C0.478142 19.0519 0.497268 19.2295 0.601093 19.3552C0.680328 19.4508 0.797814 19.5027 0.915301 19.5027C0.953552 19.5027 0.994536 19.4973 1.03279 19.4863L5.55465 18.1421C6.79781 18.7104 8.12295 18.9973 9.5 18.9973C14.7377 18.9973 19 14.735 19 9.49727C19 4.25956 14.7377 0 9.5 0ZM9.5 18.1776C8.19399 18.1776 6.93716 17.8934 5.7623 17.3333C5.6694 17.2896 5.56557 17.2814 5.46995 17.3115L1.71858 18.4262L3.0929 15.7459C3.1694 15.5956 3.14754 15.4126 3.03279 15.2842C1.60656 13.6913 0.819672 11.6339 0.819672 9.49727C0.819672 4.71038 4.71311 0.81694 9.5 0.81694C14.2869 0.81694 18.1803 4.71038 18.1803 9.49727C18.1803 14.2842 14.2869 18.1776 9.5 18.1776Z" fill="white"/>
                                    </svg>
                            </a>
                          </div>
                       {% endif %}
                      
                    </div>
                {%- endcase -%}
              </div>
            {%- endfor -%}
          </div>
        

          {%- if section.settings.show_social and has_social_icons -%}
                          <div class="custom-social-share footer-block__newsletter large-hide">
                                  <div class="footersocialicons">
                                            {%- render 'social-icons', class: 'footer__list-social' -%}
                                    </div>
                          </div>  
                      {%- endif -%}
                      {%if section.settings.need_help_footer != blank %}    
                       <div class="need-help-footer large-hide">{{ section.settings.need_help_footer }}</div>
                      {% endif %}
                      {% if section.settings.chat_with_text_footer != blank %}
                          <div class="chat-with-us-footer large-hide">
                            {{ section.settings.chat_with_text_footer }}
                         <a href="{{ section.settings.whatapp_phone_number_url }}">
                            <svg xmlns="http://www.w3.org/2000/svg" width="19" height="20" viewBox="0 0 19 20" fill="none">
                                <path d="M15.0534 11.1895L13.1818 9.82334C12.9469 9.65121 12.6573 9.58017 12.3676 9.62662C12.078 9.67034 11.8239 9.82608 11.6545 10.061L11.0753 10.8534C11.0452 10.8944 11.0562 10.8561 11.0425 10.908C11.0398 10.9135 11.007 10.9572 10.9032 10.9736C10.7283 11.0009 10.2037 10.9682 9.06982 10.061L8.96053 9.98181C7.76655 9.18673 7.57529 8.69766 7.54797 8.5228C7.5343 8.43263 7.55616 8.38892 7.55343 8.38619C7.59168 8.3616 7.58075 8.3616 7.61081 8.32334L8.19004 7.53099C8.36217 7.29602 8.43321 7.0064 8.38676 6.71679C8.34031 6.42717 8.18731 6.17307 7.95234 6.00094L6.04797 4.61023C5.56163 4.25504 4.87583 4.36159 4.52064 4.84793L3.93594 5.65121C3.41955 6.35886 3.28021 7.32608 3.57256 8.17854C4.02338 9.49274 5.17365 10.8889 6.99332 12.3315C7.01791 12.3616 7.04523 12.3862 7.07802 12.4108C7.12447 12.4436 7.17092 12.4764 7.21736 12.5064C7.26108 12.5419 7.30753 12.5747 7.35124 12.6102C7.3813 12.6348 7.41682 12.6539 7.45234 12.6676C9.3813 13.96 11.0616 14.6294 12.4523 14.6594C12.4715 14.6594 12.4879 14.6594 12.507 14.6594C13.3704 14.6594 14.1928 14.2359 14.7146 13.5173L15.2993 12.7141C15.4715 12.4791 15.5425 12.1895 15.4961 11.8998C15.4496 11.6102 15.2884 11.3616 15.0534 11.1895ZM14.6299 12.2332L14.0452 13.0365C13.6682 13.5529 13.0753 13.8507 12.4605 13.8397C11.578 13.8206 10.0398 13.4572 7.69277 11.8452C5.44414 10.102 4.6272 8.74684 4.34032 7.91351C4.13267 7.31242 4.22829 6.62935 4.59168 6.13482L5.17911 5.33154C5.22283 5.27143 5.28567 5.23318 5.35671 5.22225C5.42775 5.21132 5.50152 5.22771 5.56163 5.27143L7.466 6.66214C7.52611 6.70586 7.56436 6.7687 7.57529 6.83974C7.58622 6.91078 7.56982 6.98455 7.52611 7.04466L6.966 7.81242C6.83485 7.95449 6.7037 8.21405 6.72556 8.54192C6.76928 9.20039 7.35124 9.89165 8.49332 10.6512L8.57529 10.7086C9.65726 11.5747 10.4961 11.919 11.1354 11.7578C11.4551 11.6785 11.6627 11.4763 11.7583 11.307L12.3185 10.5392C12.4059 10.4162 12.578 10.3917 12.701 10.4791L14.5726 11.8452C14.6327 11.8889 14.6709 11.9518 14.6818 12.0228C1