 
<div class="collage-img-container">
<div class="coll-wrapper">
  {% for block in section.blocks %}
<div class="first_imgflex">
<div class="image_coll_con">
<img
                      class="desk_imgcoll"
                      srcset="{%- if block.settings.img_mobile.width >= 375 -%}{{ block.settings.img_mobile | image_url: width: 375 }} 375w,{%- endif -%}
                      {%- if block.settings.img_mobile.width >= 550 -%}{{ block.settings.img_mobile | image_url: width: 550 }} 550w,{%- endif -%}
                      {%- if block.settings.img_mobile.width >= 750 -%}{{ block.settings.img_mobile | image_url: width: 750 }} 750w,{%- endif -%}
                      {%- if block.settings.img_mobile.width >= 1100 -%}{{ block.settings.img_mobile | image_url: width: 1100 }} 1100w,{%- endif -%}
                      {%- if block.settings.img_mobile.width >= 1500 -%}{{ block.settings.img_mobile | image_url: width: 1500 }} 1500w,{%- endif -%}
                      {%- if block.settings.img_mobile.width >= 1780 -%}{{ block.settings.img_mobile | image_url: width: 1780 }} 1780w,{%- endif -%}
                      {%- if block.settings.img_mobile.width >= 2000 -%}{{ block.settings.img_mobile | image_url: width: 2000 }} 2000w,{%- endif -%}
                      {%- if block.settings.img_mobile.width >= 3000 -%}{{ block.settings.img_mobile | image_url: width: 3000 }} 3000w,{%- endif -%}
                      {%- if block.settings.img_mobile.width >= 3840 -%}{{ block.settings.img_mobile | image_url: width: 3840 }} 3840w,{%- endif -%}
                      {{ block.settings.img_mobile | image_url }} {{ block.settings.img_mobile.width }}w"
                      sizes="100vw"
                      src="{{ block.settings.img_mobile | image_url: width: 1500 }}"
                      {% if forloop.index0==0 %}loading="none"{% else %}loading="lazy"{% endif %}
                      alt="{{ block.settings.img_mobile.alt | escape }}"
                      width="{{ block.settings.img_mobile.width }}"
                      height="{{ block.settings.img_mobile.height }}"
                       >
<img
                      class="mobile_imgcoll"
                      srcset="{%- if block.settings.img_mobile.width >= 375 -%}{{ block.settings.img_mobile | image_url: width: 375 }} 375w,{%- endif -%}
                      {%- if block.settings.img_mobile.width >= 550 -%}{{ block.settings.img_mobile | image_url: width: 550 }} 550w,{%- endif -%}
                      {%- if block.settings.img_mobile.width >= 750 -%}{{ block.settings.img_mobile | image_url: width: 750 }} 750w,{%- endif -%}
                      {%- if block.settings.img_mobile.width >= 1100 -%}{{ block.settings.img_mobile | image_url: width: 1100 }} 1100w,{%- endif -%}
                      {%- if block.settings.img_mobile.width >= 1500 -%}{{ block.settings.img_mobile | image_url: width: 1500 }} 1500w,{%- endif -%}
                      {%- if block.settings.img_mobile.width >= 1780 -%}{{ block.settings.img_mobile | image_url: width: 1780 }} 1780w,{%- endif -%}
                      {%- if block.settings.img_mobile.width >= 2000 -%}{{ block.settings.img_mobile | image_url: width: 2000 }} 2000w,{%- endif -%}
                      {%- if block.settings.img_mobile.width >= 3000 -%}{{ block.settings.img_mobile | image_url: width: 3000 }} 3000w,{%- endif -%}
                      {%- if block.settings.img_mobile.width >= 3840 -%}{{ block.settings.img_mobile | image_url: width: 3840 }} 3840w,{%- endif -%}
                      {{ block.settings.img_mobile | image_url }} {{ block.settings.img_mobile.width }}w"
                      sizes="100vw"
                      src="{{ block.settings.img_mobile | image_url: width: 1500 }}"
                      {% if forloop.index0==0 %}loading="none"{% else %}loading="lazy"{% endif %}
                      alt="{{ block.settings.img_mobile.alt | escape }}"
                      width="{{ block.settings.img_mobile.width }}"
                      height="{{ block.settings.img_mobile.height }}"
                       >

</div>
<div class="Nextimage_coll_con">
<img
                      class="desk_imgcollnext"
                      srcset="{%- if block.settings.img_mobile.width >= 375 -%}{{ block.settings.img_mobile | image_url: width: 375 }} 375w,{%- endif -%}
